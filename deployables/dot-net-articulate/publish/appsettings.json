{
  
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"

    }
  },
  "Management": {
    
    "Endpoints": {
      "Enabled": true,
      "Path": "/actuators",
      "Cloudfoundry": {
        "ValidateCertificates": false
      },
      "Actuator":{
        "Exposure": {
          "Include": [
            "*"
          ]
        }
      }
    }
  },
  "Eureka": {
    "Client": {
      "Validate_certificates": false
    }
  }
}
